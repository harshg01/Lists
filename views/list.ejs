<%- include("header"); -%>

  <div class="box" id="heading">
    <h1><%= listName %></h1>
  </div>

  <div class="box">



    <% for (var i=0; i<newListItems.length; i ++) { %>
    <div class="item">
      <input type="checkbox">
      <p><%=newListItems[i]%></p>
    </div>
    <% }%>

    <form action="/" method="post">
      <input type="text" name="newItem" placeholder="Enter new item" autocomplete="off">
      <button type="submit" name="button" value = <%=listName%>>+</button>

    </form>
</div>

<%- include("footer"); -%>
